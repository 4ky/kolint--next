{
  "name": "ssr",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ./scripts/build.js",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "options": {
        "pattern": "test/**/*.test.ts"
      }
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "{projectRoot}/playwright.config.js"
      }
    }
  }
}
